# 
# 
# Environment file template used to start imawebui Docker container
#

#
# LICENSE - environment variable to accept and view license
# This is a required item
#
LICENSE=accept

#
# WEBUI_CONTAINER_NAME - Name of the MessageSight Web UI Container
#
WEBUI_CONTAINER_NAME=<%= @containerName %>

#
# WEBUI_MEMORY_SIZE - Memory size in GB used by the container
# 
WEBUI_MEMORY_SIZE=<%= @memorySize %>

#
# MESSAGESIGHT_ADMIN_HOST - IP Address of the admin endpoint
# Optional item - default is 127.0.0.1
#
MESSAGESIGHT_ADMIN_HOST=<%= @imaserver_ipAddress %>

#
# MESSAGESIGHT_ADMIN_PORT - Port number admin endpoint
# Optional item - default is 9089
#
MESSAGESIGHT_ADMIN_PORT=<%= @imaserver_adminPort %>

#
# WEBUI_PORT - Port number for the Web UI endpoint
# Optional item - default is 9087
#
WEBUI_PORT=<%= @imawebui_port %>

#
# WEBUI_HOST - Host interface for the Web UI endpoint
# Optional item - default is *
#
WEBUI_HOST=*

# Do not update the following variables
MESSAGESIGHT_SERVICE_NAME=imawebui
MESSAGESIGHT_SERVICE_VERSION=2.0

