# 
# Environment file template used to run msserver Docker container
#

#
# LICENSE - environment variable to accept and view license
# This is a required item
#
LICENSE=accept

#
# MESSAGESIGHT_MEMORY_SIZE - Memory size in GB used by the container
# This is a required item.
# The minimum allowed memory is 4 GB.
# 
MESSAGESIGHT_MEMORY_SIZE=<%= @memorySize %>

#
# MESSAGESIGHT_CONTAINER_NAME - Name of the MessageSight Container
# This is a required if configuration is on a shared volume.
# The default is IMA
# 
MESSAGESIGHT_CONTAINER_NAME=<%= @containerName %>

#
# MESSAGESIGHT_ADMIN_HOST - IP Address of the admin endpoint
# Optional item - default is All
#
MESSAGESIGHT_ADMIN_HOST=All

#
# MESSAGESIGHT_ADMIN_PORT - Port number admin endpoint
# Optional item - default is 9089
#
MESSAGESIGHT_ADMIN_PORT=<%= @adminPort %>


# Do not update the following variables
MESSAGESIGHT_SERVICE_NAME=imaserver
MESSAGESIGHT_SERVICE_VERSION=2.0

